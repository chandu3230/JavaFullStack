import java.util.Scanner;
public class array1 {
	public static void main(String args[]) {
	Scanner sc=new Scanner(System.in);
	String arr[]=new String[5];
	for(int i=0;i<5;i++) {
          System.out.println("enter the name of employee no:"+(i+1));
    arr[i]=sc.next();
	}
	for(String name:arr) {
		System.out.println(name);
	}
	}
}
