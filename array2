import java.util.Scanner;
public class array2 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		String arr[][]=new String[2][5];
		for(int i=0;i<2;i++) {
			for(int j=0;j<5;j++) {
				System.out.println("enter employee name for company"+(i+1)+",employee"+(j+1)+":");
			arr[i][j]=sc.next();				
			}
		}
		for(int i=0;i<2;i++) {
			System.out.println("company"+(i+1)+"employee names:");
			for(int j=0;j<5;j++) {
				System.out.println(arr[i][j]);
			}
		}
	}
}
