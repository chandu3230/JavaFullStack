import java.util.Scanner;
class Sum{
      public static void main(String[] args){
      Scanner sc=new Scanner(System.in);
      int a;
      int b;
      int c;
      System.out.println("enter a and b:");
      a=sc.nextInt();
      b=sc.nextInt();
      c=a+b;
      System.out.println(" sum is "+ c);
      }
}
