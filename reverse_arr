public class reverse_arr {
	public static void main(String[] args) {
		int arr[]= {10,20,5,70};
		System.out.print("b =");
		System.out.print("[");
		for(int i=arr.length-1;i>=0;i--) {
			System.out.print(arr[i]); 
	        if (i > 0) {
                System.out.print(", ");
            }
		}
		System.out.println("]");
	}
}
..............
b =[70, 5, 20, 10]
